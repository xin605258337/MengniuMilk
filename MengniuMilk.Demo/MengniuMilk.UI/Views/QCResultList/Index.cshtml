
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>质检结果录入</title>
    <script src="~/Scripts/axios.js"></script>
    <script src="~/Scripts/vue.js"></script>
    <link href="~/bootstrap-3.3.7-dist/bootstrap337/css/bootstrap.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/vue@2.5.16/dist/vue.js"></script>
</head>
<body>
    <div>
        <h3>质检结果录入</h3>
    </div>
    <div id="app" class="div-td">
        <table v-show="list.length" class="table table-striped" border="1">
            <tr>
                <th>指标项分类</th>
                <th>指标项名称</th>
                <th>指标项上限</th>
                <th>指标项下限</th>
                <th>指标项标准</th>
                <th>指标项结果值</th>
                <th>指标项结果状态</th>
            </tr>
            <tr v-for="item in QCResultList">
                <td>{{item.TargetType_Name}}</td>
                <td>{{item.Target_Name}}</td>
                <td>{{item.StandardValuesMax}}</td>
                <td>{{item.StandardValuesMax}}</td>
                <td>{{item.StandardValues}}</td>
                <td>{{item.Result}}</td>
                <td>{{item.State}}</td>
            </tr>
        </table>
    </div>
</body>
</html>
<script type="text/javascript">
    var vm = new Vue({
        el: "#add",
        data: {
            QCResultList:[]

        },
        mounted() {
            this.getQCPlanType();
        },
        methods:
        {
            //获取质检结果明细
            getQCPlanType: function () {
                axios.get('http://localhost:8103/GetQCResultLists?sampleId='+).then(
                    (response) => {
                        this.QCResultList = [];
                        for (var i = 0; i < response.data.length; i++) {
                            this.QCResultList.push(response.data[i]);
                        }
                    },
                    (response) => {
                        alert(response.status + "1");
                    }
                ).catch(function (response) {
                    alert(response);
                });

            },

        },
        computed: {
        }

    });
</script>

